<%= form_for club, :url => "#{dashboards_url}/clubs" do |f| %>
	<%= render 'shared/error_messages', :object => f.object %>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
	<p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>

	<p>
		<%= f.label :phone_number %><br />
		<%= f.text_field :phone_number %>
	</p>
	
	<p>
		<%= f.label :hours_open %>
		<%= f.text_field :hours_open %>
	</p>

	<p>
		<%= f.label :address %><br />
		<%= f.text_field :address %>
	</p>
  <p><%= f.submit %></p>
<% end %>