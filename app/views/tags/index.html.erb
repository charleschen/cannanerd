<% subtitle 'Tags'%>

<div class='sgParent sg-24'>
	<table>
	<% @tags.each do |tag| %>
		<tr>
			<td><%= tag.name %></td>
			<td><%= tag.category %></td>
			<td><%= button_to 'edit', edit_tag_path(tag), :method => :get %></td>
		</tr>
	<% end %>
	</table>
	
	<%= button_to "Create tag", new_tag_path, :method => :get %>
</div>