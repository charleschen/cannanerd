<%= form_for @club do |f| %>
	<%= render 'shared/error_messages', :object => f.object %>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
	<p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>

	<p>
		<%= f.label :address %><br />
		<%= f.text_field :address %>
	</p>
  <p><%= f.submit %></p>
<% end %>