<div class='sg-15'>
<%= form_for @user_session, :as => :user_session, :url => new_user_session_path do |f| %>
	<%= render 'shared/error_messages', :object => f.object %>
	
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p><%= f.submit %></p>
<% end %>
</div>