<ul>
		<% if permitted_to? :edit, :strains %>
			<li><%= button_to 'edit', edit_strain_path(strain), :method => :get  %></li>
		<% end %>	
<% if permitted_to? :destroy, :stock_strains %>
	<li>
	<%= form_for(club.stock_strains.find_by_strain_id(strain.id), :html => { :method => :delete}, :remote => true) do |f| %>
		<div><%= f.hidden_field :strain_id %></div>
		<div><%= f.hidden_field :club_id %></div>
		<div class='actions'>
			<%= f.submit '-', {:confirm => 'Removing this will delete ALL DATA pertaining for this strain relating to this club.  Are you sure?'} %>
		</div>
	<% end %>
	</li>
<% else %>
	<li><span class='sub'>In Inventory</span></li>
<% end%>
</ul>